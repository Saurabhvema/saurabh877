Q.6# calculator with function.py
def cal(x,y):
    a=x+y
    b=x-y
    c=x*y
    d=x/y
    t=a,b,c,d
    return t
x=int(input())
y=int(input())
print(cal(x,y))


Q.7 # function to print all armstrong no between a and b .py
def cal(x,y):
    a=x+y
    b=x-y
    c=x*y
    d=x/y
    t=a,b,c,d
    return t
x=int(input())
y=int(input())
print(cal(x,y))

Q.8# program  by function that return true if n is even and false when n is odd.
def even(n):
    if n%2==0:
        return "True"
    else:
        return "False"
n=int(input())
a=even(n)

Q.9# fabonaci with function.py
def fab(n):
    a=0
    b=1
    if n==1:
        print(a)
    else:
        print(a)
        print(b)
        for i in range(2,n):
         c=a+b
         a=b
         b=c
         print(c)
n=int(input())
fab(n)

Q.10#Sum of digits by functions.py
def sum(n):
    s=0
    for digit in n:
        s+=int(digit)
    return(s)
n=input()
print(sum(n))
