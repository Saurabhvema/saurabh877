Q.1 # Return student present and absent
def detail(roll):
    x = [23,43,22,56,77,78,90]
    if roll in x:
        print(f"Roll number {roll} is present")
    else:
        print(f"Roll number {roll} is absent")
roll = int(input("Enter roll no. ")) # 24
detail(roll)

Q.2# Factorial.py
def factorial(num):
    fact = 1
    while(num!=0):
        fact *= num
        num = num - 1
    print("Factorial is",fact)
num = int(input("Enter number "))
factorial(num)

Q.3# lower to upper.py
def response(text):
    z = text.upper()
    print(z)
text = input("Enter String: ") 
response(text)

Q.4# Count vowels-consonent.py
def count(val):
    vov = 0
    con = 0
    for i in range(len(val)):
        if val[i] in ['a','e','i','o','u']:
            vov = vov+1
        else:
            con = con + 1
    print("Count of vowels is ",vov)
    print("Count of consonant is ",con)
x = input("Enter Value: ") # pythonlobby
count(x)


